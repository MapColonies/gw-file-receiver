{
  "openapiConfig": {
    "filePath": "./openapi3.yaml",
    "basePath": "/docs",
    "rawPath": "/api",
    "uiPath": "/api"
  },
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": false
    }
  },
  "server": {
    "port": "8080",
    "request": {
      "payload": {
        "limit": "1mb"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  },
  "S3": {
    "accessKeyId": "minioadmin",
    "secretAccessKey": "minioadmin",
    "endpointUrl": "http://localhost:9000",
    "bucket": "",
    "sslEnabled": false
  },
  "FS": {
    "mountPath": "/tilesMount",
    "basePath": "tiles"
  },
  "storageProvider": "FS",
  "syncManagerUrl": "http://localhost:5555",
  "syncManagerAppKey": "",
  "httpRetry": {
    "attempts": 3,
    "delay": "exponential",
    "shouldResetTimeout": true
  }
}
